<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#F1F1F1"
    android:orientation="vertical" >

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#0E88D5"
        android:gravity="center_vertical"
        android:padding="10dp" >

        <TextView
            android:id="@+id/activity_main_query"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:text="查勘任务"
            android:textColor="#FFF"
            android:textSize="20sp" />

        <ImageView
            android:id="@+id/activity_main_help"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:src="@android:drawable/ic_menu_help" />
    </RelativeLayout>
    
    <RadioGroup 
        android:id="@+id/query_rgroup_task"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="10dp"
        android:gravity="center"
        android:orientation="horizontal"
        android:background="#FFF"
        >
        <RadioButton 
            android:id="@+id/query_rbtn_wait_submit"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@drawable/query_task_rbtn_text_selector"
            android:button="@null"
            android:background="@null"
            android:text="待提交任务"
            android:gravity="center_horizontal"
            android:layout_weight="1"
            android:checked="true"
            />
        <RadioButton 
            android:id="@+id/query_rbtn_wait_query"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@drawable/query_task_rbtn_text_selector"
            android:button="@null"
            android:gravity="center_horizontal"
            android:background="@null"
            android:layout_weight="1"
            android:text="待查勘任务"
            />
        <RadioButton 
            android:id="@+id/query_rbtn_wait_requery"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@drawable/query_task_rbtn_text_selector"
            android:button="@null"
            android:gravity="center_horizontal"
            android:background="@null"
            android:layout_weight="1"
            android:text="重新查勘任务"
            />
    </RadioGroup>
    
    <FrameLayout 
        android:id="@+id/query_framelayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="10dp"
        ></FrameLayout>


