<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#0E88D5"
        android:gravity="center_vertical"
        android:padding="10dp" >

        <TextView
            android:id="@+id/activity_main_more"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:text="个人中心"
            android:textColor="#FFF"
            android:textSize="20sp" />

        <ImageView
            android:id="@+id/activity_main_more_setting"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:src="@android:drawable/ic_menu_help" />

        <com.edu.fireeyes.views.CircleImageView
            android:id="@+id/more_iv_user_icon"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:layout_below="@id/activity_main_more"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="10dp"
            android:src="@drawable/pic" />

        <TextView
            android:id="@+id/more_tv_user_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/more_iv_user_icon"
            android:layout_centerHorizontal="true"
            android:layout_margin="10dp"
            android:text="李红银"
            android:textColor="#FFF"
            android:textSize="18sp" />
    </RelativeLayout>

    <TableLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent" >

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="wrap_content" >

            <TextView
                android:id="@+id/more_personal_tv_advice"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="15dp"
                android:layout_weight="1"
                android:drawableTop="@drawable/ic_launcher"
                android:gravity="center"
                android:text="意见反馈"
                android:textSize="12sp" />

            <TextView
                android:id="@+id/more_personal_download"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="15dp"
                android:layout_weight="1"
                android:drawableTop="@drawable/ic_launcher"
                android:gravity="center"
                android:text="文件存储位置"
                android:textSize="12sp" />

            <TextView
                android:id="@+id/more_personal_update"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="15dp"
                android:layout_weight="1"
                android:drawableTop="@drawable/ic_launcher"
                android:gravity="center"
                android:text="软件更新"
                android:textSize="12sp" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="wrap_content" 
            >
            <TextView
                android:id="@+id/more_personal_my_massage"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="15dp"
                android:drawableTop="@drawable/ic_launcher"
                android:gravity="center"
                android:layout_weight="1"
                android:text="我的消息"
                android:textSize="12sp" />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="15dp"
                android:gravity="center"
                android:layout_weight="1"
                 />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="15dp"
                android:gravity="center"
                android:layout_weight="1"
                 />
        </TableRow>
    </TableLayout>

