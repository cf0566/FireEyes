<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >
    <LinearLayout 
          android:id="@+id/item_wait_task_listview_click_linearlayout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:background="#F9F9F9"
            >
            <RadioGroup 
                android:id="@+id/item_wait_task_listview_click_radiogroup"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_weight="1"
                >
                <RadioButton 
                    android:id="@+id/item_wait_task_listview_click_rbtn_check"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="是"
                    android:textColor="#5A5A5A"
                    android:checked="true"
                    />
                <RadioButton 
                    android:id="@+id/item_wait_task_listview_click_rbtn_uncheck"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="否"
                    android:textColor="#5A5A5A"
                    />
                <RadioButton 
                    android:id="@+id/item_wait_task_listview_click_rbtn_null"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="无"
                    android:textColor="#5A5A5A"
                    />
                
                
            </RadioGroup>
            <Button 
                android:id="@+id/item_wait_task_listview_click_btn_tis"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="备注"
                android:textColor="#FFF"
                android:textSize="15sp"
                android:background="#0E88D5"
                />
            <Button 
                android:id="@+id/item_wait_task_listview_click_btn_delete"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="删除"
                android:textColor="#FFF"
                android:textSize="15sp"
                android:background="#FE1100"
                />
            
        </LinearLayout>

</LinearLayout>
